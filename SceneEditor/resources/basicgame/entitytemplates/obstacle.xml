<?xml version="1.0" encoding="UTF-8"?>
<entity-template name="obstacle">
	<component template="hitboxWall">
		<field name="noLeftCollision">false</field>
		<field name="noRightCollision">false</field>
		<field name="frictionX">0.0</field>
		<field name="frictionY">0.0</field>
		<field name="bounceCoeffY">0.0</field>
		<field name="noTopCollision">false</field>
		<field name="bounceCoeffX">0.0</field>
		<field name="noBottomCollision">false</field>
	</component>
	<component template="debug">
		<field name="a">255</field>
		<field name="r">120</field>
		<field name="b">0</field>
		<field name="g">220</field>
	</component>
	<component template="size">
		<field name="width">1200</field>
		<field name="height">200</field>
	</component>
	<component template="position">
		<field name="x">0</field>
		<field name="y">0</field>
	</component>
</entity-template>

<?xml version="1.0" encoding="UTF-8"?>
<entity-template name="player">
	<component template="position">
		<field name="x">50</field>
		<field name="y">50</field>
	</component>
	<component template="size">
		<field name="width">40</field>
		<field name="height">20</field>
	</component>
	<component template="player" />
	<component template="shooter">
		<field type="INTEGER" name="currentShootingDelay">0</field>
		<field type="INTEGER" name="shootingDelay">1</field>
		<field type="INTEGER" name="speedX">6</field>
	</component>
	<component template="texture">
		<field name="filename">player.png</field>
	</component>
</entity-template>